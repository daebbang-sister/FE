{
  "name": "daebbang-sister",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.29.2",
  "scripts": {
    "dev:client": "pnpm --filter client dev",
    "dev:admin": "pnpm --filter admin dev",
    "build:client": "pnpm --filter client build",
    "build:admin": "pnpm --filter admin build",
    "start:client": "pnpm --filter client start",
    "preview:admin": "pnpm --filter admin preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "vitest run",
    "tdd": "vitest",
    "test:ui": "vitest --ui",
    "test:client": "pnpm --filter client test",
    "test:client:tdd": "pnpm --filter client test:tdd",
    "test:admin": "pnpm --filter admin test",
    "test:admin:tdd": "pnpm --filter admin test:tdd",
    "typecheck": "tsc -b",
    "typecheck:admin": "tsc -b apps/admin",
    "typecheck:client": "tsc -b apps/client",
    "prepare": "husky install",
    "commitlint": "commitlint --config commitlint.config.js"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@eslint/js": "^9.39.1",
    "@next/eslint-plugin-next": "^16.1.6",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "husky": "^8.0.0",
    "jsdom": "^28.1.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.3.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vitest": "^4.0.18"
  }
}
